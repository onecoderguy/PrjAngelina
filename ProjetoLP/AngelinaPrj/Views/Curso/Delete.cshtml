@model AngelinaPrj.Models.Curso

@{
    ViewBag.Title = "Delete";
}

<div class="height-80 text-center">
    <h3 class="mt-5">Realmente deseja deletar este curso ?</h3>
    @using (Html.BeginForm())
    {
        @Html.AntiForgeryToken()

        <div class="form-actions no-color">
            <input type="submit" value="Sim" class="btn btn-danger" />
            @Html.ActionLink("Não", "Index", "Curso", new { @class = "btn btn-success" })
        </div>
    }
</div>
